<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shar.sharingspring.mapper.LogMapper">
	<insert id="addLog" parameterType="SystemLog">
		insert into logtable(operuser,opertype,operdetail,operdate,operresult,operip)values
		(#{operuser},#{opertype},#{operdetail},#{operdate},#{operresult},#{operip})
	</insert>
</mapper>